#https://wiki.debian.org/fstab
#PHP /etc/
/etc/PHP/5.6 /opt/PHP/5.6/etc none bind 0 0
/etc/PHP/5.5 /opt/PHP/5.5/etc none bind 0 0
/etc/PHP/5.4 /opt/PHP/5.4/etc none bind 0 0
/etc/PHP/5.3 /opt/PHP/5.3/etc none bind 0 0
#PHP /var/log
/var/log/PHP/5.6 /opt/PHP/5.6/var/log none bind 0 0
/var/log/PHP/5.5 /opt/PHP/5.5/var/log none bind 0 0
/var/log/PHP/5.4 /opt/PHP/5.4/var/log none bind 0 0
/var/log/PHP/5.3 /opt/PHP/5.3/var/log none bind 0 0
